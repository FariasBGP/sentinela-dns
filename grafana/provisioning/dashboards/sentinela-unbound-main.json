{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "stat",
      "title": "QPS (1m)",
      "id": 1,
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 0 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum(rate(unbound_answer_rcodes_total[1m]))",
          "legendFormat": "QPS",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green" },
              { "color": "red", "value": 500 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Cache Hit Ratio (1m)",
      "id": 2,
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 0 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "value",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "100 * sum(rate(unbound_cache_hits_total[1m])) / (sum(rate(unbound_cache_hits_total[1m])) + sum(rate(unbound_cache_misses_total[1m])))",
          "legendFormat": "hit %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "thresholds": {
            "mode": "percentage",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "green", "value": 85 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Exporter / Target up",
      "id": 3,
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 0 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "unbound_up",
          "legendFormat": "unbound_up",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "max by (job) (up{job=~\".*unbound.*\"})",
          "legendFormat": "up{job}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "color": "red", "text": "DOWN" }, "1": { "color": "green", "text": "UP" } } }
          ],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "red", "value": 0 }, { "color": "green", "value": 1 } ] }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "QPS por thread (aprox. via operações de cache)",
      "id": 4,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by (thread) (rate(unbound_cache_hits_total[1m]) + rate(unbound_cache_misses_total[1m]))",
          "legendFormat": "thread {{thread}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ops", "decimals": 1 },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Códigos de Resposta (RCODEs)",
      "id": 5,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by (rcode) (rate(unbound_answer_rcodes_total[1m]))",
          "legendFormat": "{{rcode}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ops", "decimals": 1 },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Respostas DNSSEC seguras (rate 1m)",
      "id": 6,
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 14 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum(rate(unbound_answers_secure_total[1m]))",
          "legendFormat": "secure",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" } }
    },
    {
      "type": "timeseries",
      "title": "Cache hits vs misses (rate 1m)",
      "id": 7,
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 14 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum(rate(unbound_cache_hits_total[1m]))",
          "legendFormat": "hits",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum(rate(unbound_cache_misses_total[1m]))",
          "legendFormat": "misses",
          "refId": "B"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" } }
    },
    {
      "type": "timeseries",
      "title": "NXDOMAIN vs SERVFAIL (rate 1m)",
      "id": 8,
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 21 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum(rate(unbound_answer_rcodes_total{rcode=\"NXDOMAIN\"}[1m]))",
          "legendFormat": "NXDOMAIN",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum(rate(unbound_answer_rcodes_total{rcode=\"SERVFAIL\"}[1m]))",
          "legendFormat": "SERVFAIL",
          "refId": "B"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" } }
    },
    {
      "type": "timeseries",
      "title": "NOERROR (sucesso) (rate 1m)",
      "id": 9,
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 21 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum(rate(unbound_answer_rcodes_total{rcode=\"NOERROR\"}[1m]))",
          "legendFormat": "NOERROR",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" } }
    }
  ],
